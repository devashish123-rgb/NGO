        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Inter"></activity>
        <activity android:name=".Nati" />
        <activity android:name=".City" />
        <activity android:name=".Comm" />
        <activity android:name=".Empo" />
        <activity android:name=".Parti" />
        <activity android:name=".Servi" />
        <activity android:name=".Chari" />
        <activity android:name=".Home" />
        <activity android:name=".Login" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
